<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find mintchoco1_controller)/config/mintchoco1_control.yaml" command="load"/>  

  <!-- load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
	output="screen" ns="/mintchoco1" args="joint_state_controller  LF_joint1_position_controller LB_joint1_position_controller RF_joint1_position_controller   RB_joint1_position_controller LF_joint2_position_controller LB_joint2_position_controller  RF_joint2_position_controller RB_joint2_position_controller LF_joint3_position_controller  LB_joint3_position_controller RF_joint3_position_controller RB_joint3_position_controller"/>


  <!-- convert joint states to TF transforms for rviz, etc -->
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"
	respawn="false" output="screen">
    <remap from="/joint_states" to="/mintchoco1/joint_states" />
  </node>

</launch>
